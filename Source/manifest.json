{
  "name": "Pinboard",
  "version": "1.4",
  "manifest_version": 2,
  "description": "A toolbar button for the Pinboard bookmarking service (http://pinboard.in).",   
  "icons": {
    "64": "icon_64.png",
    "128": "icon_128.png" 
  },
  "background":{ 
    "page":"background.html"
  },
  "browser_action": {
    "default_icon": "icon_19_a.png",
    "default_title": "Pinboard",
    "default_popup": "popup.html"
  },   
  "options_page": "options.html",
  "permissions": [ 
    "tabs", 
  "http://*/*"
    ]    
}

